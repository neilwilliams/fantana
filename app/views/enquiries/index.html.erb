<h1>Enquiries</h1>
<% if @enquiries.present? %>
  <% @enquiries.each do |e| %>
    <div class="row">
      <div class="col-sm-4">
        <%= e.created_at %>
      </div>
      <div class="col-sm-8">
        <%= e.first_name %> <%= e.last_name %>
      </div>
      <div class="col-sm-2">
        <%= link_to "Edit", edit_enquiry_path(e) %>
      </div>
    </div>
  <% end %>
<% else %>
  No enquiries to display
<% end %>
